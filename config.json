{
	"train": 
	{
		"dataroot":             "./data/",
		"savepath":				"./training/",
		"seed":                 999, 					
		"cache":           		"SavedState.pth",
		"resize":           	"256x256",
		"channels":				1, 					

		"workers":          	8,
		"batch":          		32,
		"epochs":               50,
		"warmup_epochs":		5,
		"gpus":                 "0,1", 

		"gen_input":            128, 
		"gen_features":         64,
		"dis_features":     	16, 

		"debug":                1
	},

	"generate":
	{
		"model_path":			"./training/2024-02-26-10-08-35/gen_D_G_z2_highest.pth",
		"destination":			"./results/gen_D_G_z2_highest/",
		"channels":				1,
		"gen_input":            128, 
		"gen_features":         64,
		"qty":					128,

		"gpus":                 "0,1"
	},


	"comet_dl": 
	{
		"path":                 "./comet.json"
	},


	"generator": 
	{
		"lr":        			2e-4,
		"optimizer":            "Adam",
		"beta1":                0.5,
		"saved_weights_name":   "Generator.pt"
	},


	"discriminator": 
	{
		"lr":        			2e-4,
		"optimizer":            "Adam",
		"beta1":                0.5, 
		"saved_weights_name":   "Disciminator.pt"
	}

}